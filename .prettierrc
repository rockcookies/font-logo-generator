{
	"tabWidth": 2,
	"useTabs": false,
	"singleQuote": true,
	"jsxSingleQuote": true,
	"trailingComma": "es5",
	"arrowParens": "avoid",
	"semi": false,
	"printWidth": 120,
	"quoteProps": "consistent",
	"bracketSpacing": true,
	"plugins": ["prettier-plugin-tailwindcss"],
	"overrides": [
		{
			"files": ["*.json", "*.md"],
			"options": {
				"useTabs": false,
				"tabWidth": 2
			}
		}
	]
}
